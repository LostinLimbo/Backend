<%- include ("Partials/headers") %> 

<div class="main container">
  <h1>Index Page!!</h1>

  <% blogs.forEach(blog => { %>
    <div>
      <h2><%= blog.title %></h2>
      <span><%= blog.created %></span>
      <img src="<%= blog.image %> " alt="">  
      <p><%= blog.body.substring(0, 100) %>...</p>
      <a href="/blogs/<%= blog._id %> ">Read More!</a>
    </div>
  <% }) %>
  </div>
<%- include ("Partials/footers") %> 

